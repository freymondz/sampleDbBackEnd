{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi":["warn", "always"],
        "quotes":"off",
        "import/first": "off",
        "no-multi-str":"off",
        "no-unused-vars":"warn"
    }
}
